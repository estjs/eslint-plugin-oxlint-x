{
  "name": "eslint-plugin-oxlint-x",
  "version": "0.9.20",
  "description": "",
  "type": "module",
  "keywords": [
    "eslint-plugin",
    "eslint",
    "oxlint",
    "eslint-plugin-oxlint"
  ],
  "license": "MIT",
  "homepage": "https://github.com/estjs/eslint-plugin-oxlint#readme",
  "bugs": "https://github.com/estjs/eslint-plugin-oxlint/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estjs/eslint-plugin-oxlint.git"
  },
  "author": "jiangxd <jiangxd2016@gmail.com>",
  "funding": "https://github.com/sponsors/estjs",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "node ./dist/index.mjs",
    "build": "unbuild",
    "lint": "eslint ./ --fix",
    "lint:oxlint": "node oxlint.all.js",
    "lint:oxlint:fix": "node oxlint.all.js --fix",
    "lint:oxlint:json": "node oxlint.all.js --format=json",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "release": "pnpm run build && bumpp package.json  --no-git-checks --commit --push --tag && pnpm publish --access public"
  },
  "peerDependencies": {
    "oxlint": ">=1.3.0"
  },
  "dependencies": {
    "fast-diff": "^1.3.0",
    "synckit": "^0.11.12"
  },
  "devDependencies": {
    "@estjs/eslint-config": "^2.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.2.2",
    "bumpp": "^10.1.1",
    "eslint": "^9.28.0",
    "execa": "^9.6.1",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.3",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.2.0"
  },
  "engines": {
    "node": ">=18"
  }
}
